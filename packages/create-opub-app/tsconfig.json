{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "module": "ES2015",
    "target": "es2019",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "downlevelIteration": true,
  },
  "exclude": ["templates", "dist"],
}
