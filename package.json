{
  "name": "turbo",
  "version": "1.7.4",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "postbuild": "turbo run postbuild",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,scss}\"",
    "test": "turbo run test --filter @opub-cdl/ui",
    "test:watch": "turbo run test:watch",
    "prepare": "husky install",
    "clean": "rm -rf node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "classnames": "^2.3.2",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "tsup": "^6.7.0",
    "turbo": "^1.8.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {}
}
